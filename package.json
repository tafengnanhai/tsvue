    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/dotenv-safe": "^8.1.2",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.13.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
