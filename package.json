    "happy-dom": "^8.3.2",
    "lint-staged": "^13.1.0",
    "mockjs": "^1.1.0",
    "sass": "^1.57.1",
    "scss": "^0.2.4",
    "stylelint": "^14.16.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
